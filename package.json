{
  "name": "actions-settings",
  "version": "0.0.0",
  "private": true,
  "description": "Version control a YAML file to update your repository's settings",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spenserblack/actions-settings.git"
  },
  "keywords": [
    "actions",
    "github",
    "settings"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spenserblack/actions-settings/issues"
  },
  "homepage": "https://github.com/spenserblack/actions-settings#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^20.6.2",
    "@vercel/ncc": "^0.38.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.3"
  },
  "packageManager": "pnpm@8.7.5"
